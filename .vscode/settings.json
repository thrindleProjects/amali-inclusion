{
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true,
		"source.fixAll.format": true
	},
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#c2c2c2",
		"activityBar.background": "#c2c2c2",
		"activityBar.foreground": "#15202b",
		"activityBar.inactiveForeground": "#15202b99",
		"activityBarBadge.background": "#c758c7",
		"activityBarBadge.foreground": "#15202b",
		"commandCenter.border": "#15202b99",
		"sash.hoverBorder": "#c2c2c2",
		"statusBar.background": "#a9a9a9",
		"statusBar.foreground": "#15202b",
		"statusBarItem.hoverBackground": "#8f8f8f",
		"statusBarItem.remoteBackground": "#a9a9a9",
		"statusBarItem.remoteForeground": "#15202b",
		"titleBar.activeBackground": "#a9a9a9",
		"titleBar.activeForeground": "#15202b",
		"titleBar.inactiveBackground": "#a9a9a999",
		"titleBar.inactiveForeground": "#15202b99"
	},
	"peacock.color": "#a9a9a9"
}
